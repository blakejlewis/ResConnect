<!-- views/profile.ejs -->
<!doctype html>
<html>
<head>
	<title>ResConnect Home</title>
	<link rel="stylesheet" href="//netdna.bootstrapcdn.com/bootstrap/3.0.2/css/bootstrap.min.css">
	<link rel="stylesheet" href="//netdna.bootstrapcdn.com/font-awesome/4.0.3/css/font-awesome.min.css">
	<style>
		body 		{ padding-top:80px; word-wrap:break-word; }
	</style>
</head>
<body>
<div class="container">

	<div class="page-header text-center">
		<h1><span class="fa fa-anchor"></span> Welcome Back, <%= Employee.firstName%></h1>
		<a href="/logout" class="btn btn-default btn-sm">Logout</a>
	</div>

	<div class="row">

		<!-- LOCAL INFORMATION -->
		<div class="col-sm-6">
			<div class="well">
				<h3><span class="fa fa-user"></span> Local</h3>

					<p>
						<strong>Employee ID</strong>: <%= Employee.empID %><br>
						<strong>Web ID</strong>: <%= Employee.webID %><br>
						<strong>Password</strong>: <%= Employee.password %>
					</p>

			</div>
		</div>

	</div>

</div>
	<a href="/mapping" class="btn btn-default"><span class="fa fa-user"></span> Community Mapping</a>
    <a href="/agreement" class="btn btn-default"><span class="fa fa-user"></span> Roommate Management</a>
    <a href="/proposal" class="btn btn-default"><span class="fa fa-user"></span> Program Proposals</a>
    <a href="/leadership" class="btn btn-default"><span class="fa fa-user"></span> Student Leadership</a>
</body>
</html>