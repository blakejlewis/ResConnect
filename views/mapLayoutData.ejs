    <style>
      body { 
        padding-top:20px;
        position: relative;
        text-align: center;
      }

    	.wrapper1{
      		display:grid;
      		grid-template-columns: 10% 10% 10% 10% 10% 10% 10% 10% 10% 10%;
      		/*
      		grid-column-gap:1em;
      		grid-row-gap:1em;
      		*/
      		grid-gap:1em;
    	}

      .wrapper2{
          display:grid;
          grid-template-columns: 18% 18% 18% 18% 18% 18% 18% 18%;
          justify-content: center;
          /*
          grid-column-gap:1em;
          grid-row-gap:1em;
          */
          grid-gap:1em;
      }

    	.wrapper1 > div{
      		background:#eee;
      		padding:1em;
    	}
    	.wrapper1 > div:nth-child(odd){
      		background:#ddd;
    	}

      .wrapper2 > div{
          background:#eee;
          padding:1em;
          padding-left: 1px;
          padding-right: 1px;
      }
      .wrapper2 > div:nth-child(odd){
          background:#ddd;
      }

  </style>
</head>
